class Primes:

    @staticmethod
    def first(n):
        lst = [2]
        count = 1
        number = 3
        while count < n:
            for d in range(3, int(number**0.5)+1,2):
                if number % d == 0:
                    break
            else:
                lst.append(number)
                count += 1
            number += 2
        return lst
        
        
